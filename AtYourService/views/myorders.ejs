<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
  <!-- Google Fonts -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
  <link rel="stylesheet" href="packersandmovers.css">
  <!-- Material Design Bootstrap -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/css/mdb.min.css" rel="stylesheet">
  <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
  <title>AYS-Cart</title>
  <link rel="icon" type="img" href="/logo.jpg">
</head>

<body>
  <!--Navbar-->
  <%- include('./partials/nav.ejs') %>
    <div class="row align-items-center justify-content-center">
      <h1 class="row mt-3 mb-3 fw-bold" style="width:60%;justify-content:center;color: rgb(43, 101, 226);">My orders</h1>
      <% orders.forEach(element=> { %>
        <% if (element.customerid==cus1._id) { %>
          <!-- <h1> <%= element.employeeid %> </h1>
          <h1> <%= element.address %> </h1>  -->
          <div class="card ml-3 mr-3 mb-1" style="width:60%;justify-content:center">
            <div class="row">
              <!-- <div class="card col-md-10 m-0 p-0"> -->
                <div class="row">
                  <div class="col-auto d-flex align-items-center" id="order-image" style="margin-left:20px;">

                    <!-- <img src="/ays.jpg" class="img-thumbnail " style="width:100px; height:100px;"> -->

                    <% if(element.servicename=="hair cut" ){ %>
                      <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S11.jpg" alt=""
                          style="width:180px; height:100px;"></div>
                      <% }%>
                        <% if(element.servicename=="facials & cleanup" ){ %>
                          <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S12.jpg" alt=""
                              style="width:180px; height:100px;"></div>
                          <% }%>
                            <% if(element.servicename=="Massages, Pedicure & Manicure" ){ %>
                              <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S13.jpg" alt=""
                                  style="width:180px; height:100px;"></div>
                              <% }%>
                                <!-- house cleanig -->
                                <% if(element.servicename=="Home cleaning" ){ %>
                                  <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S21.jpg" alt=""
                                      style="width:180px; height:100px;"></div>
                                  <% }%>
                                    <% if(element.servicename=="house cleaning" ){ %>
                                      <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S22.jpg"
                                          alt="" style="width:158px; height:100px;"></div>
                                      <% }%>

                                        <% if(element.servicename=="House organizers" ){ %>
                                          <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img src="/S23.jpg"
                                              alt="" style="width:180px; height:100px;"></div>
                                          <% }%>
                                            <!-- Aplliances -->

                                            <% if(element.servicename=="Ac repair" ){ %>
                                              <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img
                                                  src="/S31.jpg" alt="" style="width:180px; height:100px;"></div>
                                              <% }%>
                                                <% if(element.servicename=="Fridge repair" ){ %>
                                                  <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img
                                                      src="/S32.jpg" alt="" style="width:180px; height:100px;"></div>
                                                  <% }%>
                                                    <% if(element.servicename=="Washing machine repair" ){ %>
                                                      <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img
                                                          src="/S33.jpg" alt="" style="width:180px; height:100px;">
                                                      </div>
                                                      <% }%>
                                                        <!-- packers and movers -->
                                                        <% if(element.servicename=="Domestic moving" ){ %>
                                                          <div class="col-auto mt-3 mb-3" style="margin-left:30px;"><img
                                                              src="/S41.jpg" alt="" style="width:180px; height:100px;">
                                                          </div>
                                                          <% }%>
                                                            <% if(element.servicename=="Car moving" ){ %>
                                                              <div class="col-auto mt-3 mb-3" style="margin-left:30px;">
                                                                <img src="/S42.jpg" alt=""
                                                                  style="width:180px; height:100px;"></div>
                                                              <% }%>
                                                                <% if(element.servicename=="Office moving" ){ %>
                                                                  <div class="col-auto mt-3 mb-3"
                                                                    style="margin-left:30px;"><img src="/S43.jpg" alt=""
                                                                      style="width:180px; height:100px;"></div>
                                                                  <% }%>


                  </div>
                  <div class="col-auto" style="margin:20px;">
                    <h3 class="title" style="color: rgba(20, 128, 223, 0.995);">
                    <b>Service type :</b>  <%= element.servicename %>
                    </h3>
                    <p>
                    Location: <%= element.address %>
                    </p>
                    <a href="/orders/<%= element._id %>">
                         <pre>Order details <i class="bi bi-chevron-right black-text" style="font-size:12px;"></i></pre>
                    </a>
                  </div>
                </div>
              </div>


              <!-- <div class="col-md-2 d-flex align-items-center">
                <a href="/orders/<%= element._id %>">
                  <i class="bi bi-chevron-right black-text" style="font-size:45px;"></i>
                </a>
              </div> -->
              
            <!-- </div> -->
          </div>
          <% } %>
            <%}) %>
              <div class="mt-3 mb-3">
                <hr>
              </div>
              <div id="services" class="container" style="width: 80%;">
                <%- include('./partials/services.ejs')%>
              </div>
    </div>
    </div>
    <!--Footer-->
    <%- include('./partials/footer.ejs') %>
      <!-- JQuery -->
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
      <!-- Bootstrap tooltips -->
      <script type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
      <!-- Bootstrap core JavaScript -->
      <script type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js"></script>
      <!-- MDB core JavaScript -->
      <script type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/js/mdb.min.js"></script>
      <!-- Optional JavaScript; choose one of the two! -->
      <!-- Option 1: Bootstrap Bundle with Popper -->
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
      <!-- Option 2: Separate Popper and Bootstrap JS -->
      <!--
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
    -->
</body>

</html>